    "[ini]": {
      // https://marketplace.visualstudio.com/items?itemName=maptz.regionfolder
      // "foldStartRegex": ";[\\s\\w]*{{{[\\s]*(.*)",
      // "foldEndRegex": ";[\\s]*}}}",
      "foldStartRegex": "\\s*;\\s* {{{\\s*.*",
      "foldEndRegex": "\\s*; }}}\\s*.*", 
      "foldStart": "; [NAME] {{{",
      "foldEnd": "; }}}",
      "disableFolding": false
    }
  },
  "javascript.inlayHints.variableTypes.enabled": true,
