{
  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 1,
  "window.customTitleBarVisibility": "never",
  "terminal.integrated.fontSize": 18,
  "terminal.external.linuxExec": "kitty",
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.stickyScroll.enabled": false,
  "terminal.integrated.env.linux": {},
  "terminal.integrated.fontFamily": "'Fantasque Sans Mono', 'FiraCode Nerd Font Mono', 'Fira Code', monospace",
  "editor.fontFamily": "'Fantasque Sans Mono', 'FiraCode Nerd Font Mono', 'JetBrains Mono', 'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.stickyScroll.enabled": true,
  "editor.smoothScrolling": true,
  "editor.renderWhitespace": "selection",
  "editor.guides.indentation": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontSize": 18,
  "editor.minimap.enabled": false,
  "editor.fontLigatures": true,
  "editor.renderLineHighlight": "none",
  "editor.wordWrap": "on",
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  // "editor.lineNumbers": "relative",
  "apc.activityBar": {
    "size": 0,
  },
  "apc.statusBar": {
    "height": 0
  },
  "apc.parts.font.family": {
    "titlebar": "Fantasque Sans Mono",
    "auxiliarybar": "Fantasque Sans Mono",
    "banner": "Fantasque Sans Mono",
    "extension-editor": "Fantasque Sans Mono",
    "monaco-menu": "Fantasque Sans Mono",
    "panel": "Fantasque Sans Mono",
    "settings-body": "Fantasque Sans Mono",
    "sidebar": "Fantasque Sans Mono",
    "statusbar": "Fantasque Sans Mono",
    "tabs": "Fantasque Sans Mono",
  },
  "apc.stylesheet": {
    ".editor .title": "background: transparent !important",
    ".editor .scrollbar .slider": "visibility: hidden",
    ".titlebar-container": "height: 400px",
    ".actions-container .action-item": "visibility: hidden",
    ".window-icon": "visibility: hidden",
    ".title.tabs": "display: none",
    ".tabs-container": "display: none !important",
    ".editor .scrollbar:hover .slider": "visibility: initial",
    "body > .file-icons-enabled.enable-motion.monaco-workbench.sidebar-right > .monaco-grid-view > div > div > .monaco-scrollable-element > .split-view-container > div:nth-child(1)": "display: none !important;",
    "body > .file-icons-enabled.enable-motion.monaco-workbench.sidebar-right > .monaco-grid-view > div > div > .monaco-scrollable-element > .split-view-container > div:nth-child(3)": "top: 12px !important;",
  },
  "workbench.colorCustomizations": {},
  "workbench.tips.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "off",
  "files.exclude": {
    "**/node_modules/": true,
    "**/.mypy_cache": true,
    "**/*.egg-info": true,
    "**/__pycache__": true,
    "**/.venv/": true,
    "**/.ipynb_checkpoints/": true,
    "**/yarn.lock": false,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.tslint": "explicit",
      "source.fixAll.eslint": "explicit"
    }
  },
  "vite.autoStart": false,
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "Vue.volar"
  },
  "[sql]": {
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.fixAll.tslint": "explicit",
      "source.fixAll.eslint": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.fixAll.tslint": "explicit",
      "source.fixAll.eslint": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.tabSize": 4,
    // "editor.formatOnSave": true,
    // "editor.formatOnType": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript][javascriptreact][typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.tslint": "explicit",
      "source.fixAll.eslint": "explicit"
    }
  },
  "[gdscript]": {
    "editor.defaultFormatter": "EddieDover.gdscript-formatter-linter"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.tslint": "explicit",
      "source.fixAll.eslint": "explicit"
    }
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "eslint.validate": [
    "vue",
    "html",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "gitlens.hovers.currentLine.over": "line",
  "emmet.triggerExpansionOnTab": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "emulator.emulatorPath": "~/Android/Sdk/emulator",
  "files.associations": {
    "*.graphql": "graphql",
    "*.stg": "lua",
    ".gcloudignore": "ignore"
  },
  "git.autofetch": true,
  "redhat.telemetry.enabled": false,
  "python.languageServer": "Pylance",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "default",
    "*.pdf": "default"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "typescript.validate.enable": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true,
    "python": false,
    "clojure": true
  },
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "material-icon-theme",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "maptz.regionfolder": {
    "[ini]": {
      // https://marketplace.visualstudio.com/items?itemName=maptz.regionfolder
      // "foldStartRegex": ";[\\s\\w]*{{{[\\s]*(.*)",
      // "foldEndRegex": ";[\\s]*}}}",
      "foldStartRegex": "\\s*;\\s* {{{\\s*.*",
      "foldEndRegex": "\\s*; }}}\\s*.*",
      "foldStart": "; [NAME] {{{",
      "foldEnd": "; }}}",
      "disableFolding": false
    }
  },
  "javascript.inlayHints.variableTypes.enabled": true,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.suggest.jsdoc.generateReturns": false,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "debug.inlineValues": "on",
  "gitlens.advanced.messages": {
    "suppressRebaseSwitchToTextWarning": true
  },
  "githubPullRequests.pullBranch": "never",
  "errorLens.gutterIconsEnabled": true,
  "react-native-tools.showUserTips": false,
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.tsserver.log": "off",
  "debug.javascript.autoAttachFilter": "smart",
  "workbench.editor.empty.hint": "hidden",
  "debug.toolBarLocation": "commandCenter",
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.sideBar.location": "right",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "workbench.tree.enableStickyScroll": true,
  "explorer.confirmDelete": false,
  "console-ninja.featureSet": "Community",
  "excalidraw.theme": "auto",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "svelte.enable-ts-plugin": true,
  "calva.paredit.defaultKeyMap": "strict",
  "github.copilot.editor.enableAutoCompletions": true,
  "yaml.schemas": {
    "file:///home/john/.vscode/extensions/atlassian.atlascode-3.0.2/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "workbench.colorTheme": "GitHub Dark"
}
