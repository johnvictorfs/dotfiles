  "explorer.confirmDragAndDrop": false,
  "files.maxMemoryForLargeFilesMB": 6096,
  "files.autoSave": "off",
  "files.exclude": {
    "**/node_modules/": true,
    "**/.mypy_cache": true,
    "**/*.egg-info": true,
    "**/__pycache__": true,
    "**/.venv/": true,
    "**/.ipynb_checkpoints/": true,
